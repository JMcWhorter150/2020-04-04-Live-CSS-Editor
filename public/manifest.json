{
  "manifest_version": 2,

  "name": "Joe's CSS Editor",
  "description": "This extension allows real-time editing of web pages using React",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Joe's CSS Editor"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-0L9v3EXRKdMQg1gKoKoptUSV3p14UMg7cnkyEv55nO0='; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
